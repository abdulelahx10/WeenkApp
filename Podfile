    pod 'FirebaseUI/Google'
    pod 'FirebaseUI/Twitter'
    pod 'TwitterKit'
    pod 'GoogleSignIn'
    pod 'SwiftyJSON'
    pod 'Mapbox-iOS-SDK'
    pod 'MapboxNavigation'
    pod 'ARCL'
end

